{% extends '../../../../../../templates/base.html.twig' %}

{% block title %}FAQ{% endblock %}

{% block body %}
    <h1>FAQ</h1>


    {% for faqCategory in faqCategories %}
        <h2>{{ faqCategory.name }}</h2>

        {% for faqQuestion in faqCategory.questions %}
            <h3>{{ faqQuestion.question|raw  }}</h3>
            <p>{{ faqQuestion.answer|raw  }}</p>
        {% endfor %}
    {% endfor %}
{% endblock %}
